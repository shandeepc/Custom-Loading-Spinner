<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE Plugin PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<Plugin displayName="Custom Loading Spinner" minSystemVersion="8.0" name="CustomLoadingSpinner" rightRequired="" version="1.0">
  <Attributes>
    <Map>
      <entry key="minUpgradableVersion" value="1.0"/>
      <entry key="snippets">
        <value>
          <List>
            <Snippet regexPattern=".*" rightRequired="">
              <Scripts>
                <String>ui/js/customloadingspinner.js</String>
              </Scripts>
              <StyleSheets>
                <String>ui/css/customloadingspinner.css</String>
              </StyleSheets>
            </Snippet>
          </List>
        </value>
      </entry>
    </Map>
  </Attributes>
</Plugin>
